/*python*/
/*создать функцию насоса(насос это пример из пахта), 
внутри которой результатом вызова этой функции будут другие функции, 
которые вернут тебе результат другой функции*/
def find_h(k):
return f"Set: k\n Found: h = {-k} ед."

def find_k(h):
return f"Set: h\n Found: k = {-h} ед."

def pump(*funcs):
def wrapper(h=0, k=0):
for func in funcs:
if "centrifugal" in func.__name__:
print("Current pump: centrifugal")
print(find_h(func(h, k)))
else:
print("Current pump: Plunger pump")
print(find_k(func(h, k)))
print()
return wrapper

@pump
def centrifugal_pump(h=0, k=0):
return h if h else k

@pump
def plower_pump(h=0, k=0):
return h if h else k

centrifugal_pump(k=2)
plower_pump(h=12)


pumped_cube = pump(calculate_volume, calculate_length)
pumped_cube(125)
